{
  "name": "ghost-rebrand",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "dev": "next dev --turbo",
    "dev:with-worker": "concurrently \"npm run start\" \"npm run worker\"",
    "prod:with-worker": "concurrently \"npm run start\" \"npm run worker\" \"npm run worker\", \"npm run worker\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "worker": "tsx src/scripts/worker.ts"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.12.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@tryghost/admin-api": "^1.13.15",
    "@vitejs/plugin-react": "^4.4.1",
    "bull": "^4.16.5",
    "dotenv": "^16.5.0",
    "next": "^15.2.3",
    "node-mocks-http": "^1.17.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "redis": "^5.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/bull": "^3.15.9",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.15",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vitest": "^3.1.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "npm@10.8.2"
}
